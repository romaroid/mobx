    "jsonwebtoken": "^8.2.1",
    "lodash.merge": "^4.6.1"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
